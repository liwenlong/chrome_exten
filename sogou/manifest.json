{
  "manifest_version": 2,
  "name": "搜狗广告",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions":[
    "activeTab"
  ]
}
